    </messagre>

    <message name="checkbalanceRequest">
        <part name="accountnumber" type="xsd:interger"/>
    </message>
    <message name="checkbalanceResponse">
        <part name="balance" type="xsd:float"/>
    </message>

    <portType name="BankAccountPortType" >
        <opearation name="deposit">
            <input message="DepositRequest"/>
            <output message="DepositResponse"/>
        </opearation>
        <opearation name="withdrow">
            <input message="WithdroeRequest"/>
            <output message="withdrowResponse"/>
        </opearation>
        <opearation name="checkbalance">
